@model IEnumerable<MovieApp.Entity.UserModel>
@{
    ViewData["Title"] = "ShowUserDetails";
}

<h1>ShowUserDetails</h1>
<hr />
<form>
<table class="table table-striped">
  <tr><td>Sr.No</td><td>Id</td> <td>FirstName</td> <td>LastName</td> <td>Email</td><td>MobileNo</td><td>Edit</td><td>Delete</td></tr>
    @{
    
    foreach(var item in Model)
    {
       <tr><td><input type="checkbox"/></td><td>@item.UserId</td><td>@item.FirstName </td><td>@item.LastName</td>
       <td>@item.Email</td>
       <td>@item.Mobile</td>
       <td><a asp-action="EditUser" asp-controller="User" asp-route-UserId="@item.UserId">Edit</a></td>
       <td><a asp-action="Delete" asp-controller="User" asp-route-UserId="@item.UserId">Delete</a></td>
       </tr>

    }

}

</table>

</form>