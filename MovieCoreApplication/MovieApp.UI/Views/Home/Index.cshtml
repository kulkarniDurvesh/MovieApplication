@model IEnumerable<MovieApp.Entity.MovieModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<h1 style="color:red">Movies</h1>
<div class="row row-cols-1 row-cols-md-3 g-4">
  @foreach (var item in Model)
 {
  <div class="col-4">
    <div class="card h-100">
      <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">@item.MovieName</h5>
        <button class="btn btn-outline-success"><a asp-action="AddBookingItem" asp-controller="BookingMovie" asp-route-MovieId="@item.MovieId">Book Now</a></button>
    </div>
    </div>
  </div>
  }
</div>


