@model IEnumerable<Teacher.Entity.Master>
@{
    ViewData["Title"] = "ShowAllTeacher";
}

<h1>ShowAllTeacher</h1>
<hr />
<form>
<table class="table table-striped">
  <tr><td></td><td>Id</td> <td>Name</td> <td>Skills</td> <td>TotalStudent</td><td>Salary</td></tr>
    @{
    
    foreach(var item in Model)
    {
       <tr><td><input type="checkbox" /></td><td>@item.Id</td><td>@item.Name </td><td>@item.Skills</td><td>@item.TotalStudent</td><td>@item.Salary</td></tr>

    }

}

</table>
</form>
